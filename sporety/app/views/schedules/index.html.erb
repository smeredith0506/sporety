<h1>Listing schedules</h1>

<table>
  <thead>
    <tr>
      <th>Players</th>
      <th>Locations</th>
      <th>Facilities</th>
      <th>Matches</th>
      <th>Game</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @schedules.each do |schedule| %>
      <tr>
        <td><%= schedule.players %></td>
        <td><%= schedule.locations %></td>
        <td><%= schedule.facilities %></td>
        <td><%= schedule.matches %></td>
        <td><%= schedule.game %></td>
        <td><%= link_to 'Show', schedule %></td>
        <td><%= link_to 'Edit', edit_schedule_path(schedule) %></td>
        <td><%= link_to 'Destroy', schedule, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Schedule', new_schedule_path %>
